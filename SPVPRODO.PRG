#include "inkey.ch"
private W_OLDCOR := setcolor(), W_SUB
private W_VET := { "letra_e()"   ,;
                   "letra_s2()"  ,;
                   "letra_d()"   ,;
                   "letra_i()"   ,;
                   "letra_p()"   ,;
                   "letra_s1()"  ,;
                   "letra_e()"   ,;
                   "letra_s2()"  ,;
                   "letra_d()"   ,;
                   "letra_i()"   ,;
                   "letra_p()"   ,;
                   "letra_s1()"  }

setcursor(0)
telaback()

setcolor("R*+/B,R/B+,B")
for x := 1 to 6
   eval( {|| &(W_SUB := W_VET[X]) } )
   if inkey(.5) = K_ESC; exit; end
next

if lastkey() != K_ESC
   setcolor("B+/B,R/B+,B")
   for x := 1 to 6
      eval( {|| &(W_SUB := W_VET[X]) } )
      if inkey(.5)=K_ESC; exit; end
   next
   inkey(4)
end

setcolor(W_OLDCOR)
setcursor(1)
return

